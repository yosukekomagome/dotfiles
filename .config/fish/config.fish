if status is-interactive
    # Commands to run in interactive sessions can go here
    alias vim='nvim'
    alias debugchrom='/Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome --remote-debugging-port=9222'
    alias ll='eza -l -F --icons --git --sort=type --time-style iso'
    alias lla='eza -a -l -F --icons --git --sort=type --time-style iso'
end

